<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="model.Departamento" table="Departamento">
        <id name="id" column="id" >
            <generator class="native"></generator>
        </id>
        <property name="nombre" type="string" column="nombre"></property>
        <property name="ubicacion" type="string" column="ubicacion"></property>
        <property name="email" type="string" column="email"></property>
        <property name="telefono" type="string" column="telefono"></property>



    </class>

</hibernate-mapping>
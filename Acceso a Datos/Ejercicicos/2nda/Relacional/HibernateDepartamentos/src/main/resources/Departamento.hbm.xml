<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibertante Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.example.model.Departamento" table="Departamento">
        <id name="id" column="id">
            <generator class="native"></generator>
        </id>
        <property name="nombre" type="string" column="nombre"/>
        <property name="ubicacion" type="string" column="ubicacion"/>
        <property name="email" type="string" column="email"/>
        <property name="telefono" type="string" column="telefono"/>


    </class>
</hibernate-mapping>